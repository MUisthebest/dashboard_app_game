{
  "version": 2,
  "outputDirectory": "dist/my-angular-app/browser",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.js)",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*\\.css)",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/(.*\\.mjs)",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*\\.wasm)",
      "headers": {
        "Content-Type": "application/wasm"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}